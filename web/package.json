    "chokidar": "^3.5.3",
    "fs-extra": "^11.1.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.13",
    "@types/multer": "^1.4.7",
    "@types/ws": "^8.5.5",
    "vite": "^4.4.0",
    "concurrently": "^8.2.0",
    "nodemon": "^3.0.1",
    "eslint": "^8.45.0",
    "jest": "^29.6.0",
